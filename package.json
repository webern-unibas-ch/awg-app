        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "^2.2.1",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "^2.1.0",
        "lint-staged": "^15.5.1",
        "prettier": "^3.5.3",
        "source-map-explorer": "^2.5.3",
        "typescript": "~5.6.3",
        "typescript-eslint": "^8.31.0",
        "webpack-bundle-analyzer": "^4.10.2"
    },
    "engines": {
        "node": ">= 20.13.1",
        "npm": ">= 10.5.2",
