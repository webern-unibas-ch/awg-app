        "eslint-plugin-deprecation": "^3.0.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jsdoc": "^50.4.3",
        "eslint-plugin-prettier": "^5.2.1",
        "gzipper": "^7.2.0",
        "husky": "^9.1.6",
        "jasmine-core": "~5.4.0",
        "karma": "~6.4.4",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "^2.2.1",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "^2.1.0",
        "lint-staged": "^15.2.10",
